#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int nums[10];
int i = 0;
int espacoTotal;
int espacoVetorPrincipal;
int numsTemporario[10];
int valorTemporario;

int vetorTemporario(espacoTotal){
	int numsTemporario[10];
    int valorTemporario = 0;
	for(int i = 0; i <= espacoTotal; i++){
		numsTemporario[i] = valorTemporario;
        valorTemporario++;
         printf("i = %d\n", i);
         printf("valorTemporario = %d\n", valorTemporario);
    }
	//Criacao do vetor temporario ordenadao numsTemporario = 0, 1, 2, 3
}

