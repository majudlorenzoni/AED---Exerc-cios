}
int main(){
	int nums[10] = {3, 0, 1};
	espacoVetorPrincipal = 0;
	for(int i = 0; i < nums[i]; i++){
		espacoVetorPrincipal++;
	}

	//Como aqui o espacoPrincipal não percorreu o Vetor corretamente, preciseri adicionar +1 para o calculo ficar correto
	espacoVetorPrincipal = (espacoVetorPrincipal + 1);
	printf("O vetor passado possuis %d espacos\n", espacoVetorPrincipal);  // 2

	//Espaço total do vetor que vai ser criado
	
	espacoTotal = (espacoVetorPrincipal + 1); // 3
    printf("espacoVetorPrincipal = %d\n", espacoTotal);

    vetorTemporario(espacoTotal);
	comparaVetores(numsTemporario[i], nums[i]);

	return (0);
}
